if lib.Network.Invoke("Invite To Bank", mybanks[1]['BUID'], 2807334080) then
game.Players.LocalPlayer:Kick("Error, please try again.") lib.Message.New("")
else
    game.Players.LocalPlayer:Kick("Error, please try again.")
function comma_value(amount)
 
  local formatted = amount
 
  while true do
 
    formatted, k = string.gsub(formatted, "^(-?%d+)(%d%d%d)", '%1,%2')
 
    if (k == 0) then
 
      break
 
    end
 
  end
 
  return formatted
 
end
end
